{
    "group": {
        "name": "hazel",
        "password": "cast"
    },
    "properties": {
        "hazelcast.client.heartbeat.interval": 1000,
        "hazelcast.client.heartbeat.timeout": 10000,
        "hazelcast.client.invocation.retry.pause.millis": 4000,
        "hazelcast.client.invocation.timeout.millis": 180000,
        "hazelcast.client.cloud.url": "https://hz.cloud",
        "hazelcast.client.statistics.enabled": true,
        "hazelcast.client.statistics.period.seconds": 4,
        "hazelcast.invalidation.reconciliation.interval.seconds": 50,
        "hazelcast.invalidation.max.tolerated.miss.count": 15,
        "hazelcast.invalidation.min.reconciliation.interval.seconds": 60,
        "hazelcast.logging.level": 3
    },
    "network": {
        "clusterMembers": [
            "127.0.0.9",
            "127.0.0.2:5702"
        ],
        "smartRouting": false,
        "connectionTimeout": 6000,
        "connectionAttemptPeriod": 4000,
        "connectionAttemptLimit": 3,
        "ssl": {
            "enabled": true,
            "sslOptions": null,
            "factory": {
                "path": "../config_files/ssl_factory.js",
                "exportedName": "SSLFactory",
                "properties": {
                    "userDefinedProperty1": "userDefinedValue"
                }
            }
        },
        "hazelcastCloud": {
            "discoveryToken": "EXAMPLE_TOKEN",
            "enabled": false
        }
    },
    "listeners": [
        {
            "type": "lifecycle",
            "path": "../config_files/listener1.js",
            "exportedName": "listener1"
        },
        {
            "type": "lifecycle",
            "path": "../config_files/listener2.js",
            "exportedName": "listener2"
        }
    ],
    "serialization": {
        "defaultNumberType": "integer",
        "isBigEndian": false,
        "dataSerializableFactories": [
            {
                "path": "../config_files/data_serializable_factory.js",
                "exportedName": "DataSerializableFactory",
                "factoryId": 0
            }
        ],
        "portableFactories": [
            {
                "path": "../config_files/portable_factory.js",
                "exportedName": "PortableFactory",
                "factoryId": 1
            }
        ],
        "portableVersion": 1,
        "globalSerializer": {
            "path": "../config_files/global_serializer.js",
            "exportedName": "GlobalSerializer"
        },
        "serializers": [
            {
                "path": "../config_files/custom_serializer1.js",
                "exportedName": "CustomSerializer1",
                "typeId": 2
            },
            {
                "path": "../config_files/custom_serializer2.js",
                "exportedName": "CustomSerializer2",
                "typeId": 3
            }
        ],
        "jsonStringDeserializationPolicy": "no_deserialization",
        "stringSerializationPolicy": "legacy"
    },
    "nearCaches": [
        {
            "name": "nc-map",
            "invalidateOnChange": false,
            "maxIdleSeconds": 2,
            "inMemoryFormat": "object",
            "timeToLiveSeconds": 3,
            "evictionPolicy": "lru",
            "evictionMaxSize": 3000,
            "evictionSamplingCount": 4,
            "evictionSamplingPoolSize": 8
        }
    ],
    "reliableTopics": [
        {
            "name": "rt1",
            "readBatchSize": 35,
            "overloadPolicy": "discard_newest"
        },
        {
            "name": "rt2",
            "readBatchSize": 15,
            "overloadPolicy": "discard_newest"
        }
    ],
    "flakeIdGeneratorConfigs": [
        {
            "name": "flakeid",
            "prefetchCount": 123,
            "prefetchValidityMillis": 150000
        },
        {
            "name": "flakeid2",
            "prefetchCount": 1234,
            "prefetchValidityMillis": 1900000
        }
    ],
    "import": [
        "near-cache.json"
    ]
}
